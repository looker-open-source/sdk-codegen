{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "tmp/compiled-typescript",
		"baseUrl": ".",
		"types": ["node"],
		"paths": {
			"*": [
				"node_modules/*"
				]
			}
		},
		"include": [
			"src/**/*"
		]
	}
